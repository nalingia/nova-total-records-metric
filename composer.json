{
    "name": "nalingia/total-records",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "autoload": {
        "psr-4": {
            "Nalingia\\TotalRecords\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nalingia\\TotalRecords\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../nova"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
